<html lang="en">

<head>
    <title>Echo Lab</title>
</head>

<body>
    <h1>EchoLab Overview</h1>
    <!-- <pre><%= JSON.stringify(data, null, 2) %></pre> -->
    <h2>Figures</h2>
    <ul>
    <% data.figures.forEach(figure => { %>
        <li><strong><%= figure.name %></strong> - <%= figure.lifespan %></li>
    <% }); %>
    </ul>

    <h2>Titles</h2>
    <ul>
    <% data.titles.forEach(title => { %>
        <li><%= title.title %> - <%= title.type%></li>
    <% }); %>
    </ul>

    <h2>Concepts</h2>
    <ul>
    <% data.concepts.forEach(concept => { %>
        <li><strong><%= concept.term %></strong> - <%= concept.definition %></li>
    <% }); %>
    </ul>
    

</body>



</html>